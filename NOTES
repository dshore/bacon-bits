# svn on linux
# create new repository
svnadmin create /usr/local/svn/games

# set permissions on repository
su
cd /usr/local/svn
chown -R webadmin games/
chgrp -R webadmin games/
chmod -R g+w,o= games/

# create empty project
cd ~/tmp
mkdir games
cd games
mkdir bacon

svn import . file:///usr/local/svn/games -m "First import."

# on local machine
svn co http://www.mapspace.com/svn/games/bacon

