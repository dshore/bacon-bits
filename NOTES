####################################################
# grid game
####################################################
head word is randomly chosen from all nodes
tail word has to be at least two nodes away
> challenge: find shortest path from head to tail

(i)nsert, (d)elete, (m)ove, (p)rint

insert new word after N: i N word
delete word N: d N
move word: m N N1
print grid: p g
print shortest path: p s

all links

synonym: car :: auto
hypernym/hyponym: car :: vehicle
antonym: hot :: cold
homonym: meet :: meat
collocate: fountian :: pen
collocate/synonym: rabbit :: ears :: antenna
meronym/holonym: wheel :: car

bacon :: cruise
---------------------------------------------------------------------------------------------
bacon :: Synset('bacon.n.01') <hypernym> Synset('cut_of_pork.n.01') :: cut of pork
cut of pork :: Synset('cut_of_pork.n.01') <part_holonym> Synset('pork.n.01') :: pork 
pork :: Synset('pork.n.01') <hypernym> Synset('meat.n.01') :: meat
meat :: Synset('meat.n.01') <hyponym> Synset('beef.n.02') :: beef
beef :: Synset('beef.n.02') <part_holonym> Synset('cattle.n.01') :: cattle
cattle :: Synset('cattle.n.01') <hyponym> Synset('bullock.n.02') :: steer
steer :: Synset('bullock.n.02') <homonym> Synset('steer.v.01') :: steer
steer :: Synset('steer.v.01') <hyponym> Synset('navigate.v.03') :: navigate
navigate :: Synset('navigate.v.03') <homonym> Synset('voyage.v.01') :: navigate
navigate :: Synset('voyage.v.01') <hyponym> Synset('cruise.v.04') :: cruise

pen :: pea
---------------------------------------------------------------------------------------------
pen :: Synset('pen.*') <collocate> Synset('fountain.*') :: fountain
fountain :: Synset('fountain.*') <collocate> Synset('soda.*') :: soda
soda :: Synset('soda.*') <collocate> Synset('water.*') :: water
water :: Synset('water.*') <homonym> Synset('urine.n.01') :: water
water :: Synset('urine.n.01') <synonym> Synset('urine.n.01') :: pee
pee :: Synset('pee.*') <homonym> Synset('pea.*') :: pea

apple :: dust
---------------------------------------------------------------------------------------------
apple :: Synset('apple.n.01') <collocate> Synset('tree.v.02') :: tree
tree :: Synset('tree.v.02') <hypernym> Synset('plant.v.01') :: plant
plant :: Synset('plant.v.01') <derivation> Synset('planting.n.03') :: planting
